{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "city": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "city"
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "fullAddress": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "imageAlt": {
      "type": "string"
    },
    "heroSection": {
      "type": "component",
      "repeatable": true,
      "component": "hero.hero-home"
    },
    "block": {
      "type": "dynamiczone",
      "components": [
        "slice.slice-rich-text-with-info",
        "slice.content-list-section",
        "content-block.center-content-block",
        "content-block.testimonial-block",
        "slice.logo-block-with-testimonial",
        "slice.carousel"
      ]
    }
  }
}
