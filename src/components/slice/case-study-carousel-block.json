{
  "collectionName": "components_slice_case_study_carousel_blocks",
  "info": {
    "displayName": "CaseStudy Carousel Block",
    "icon": "book-open"
  },
  "options": {},
  "attributes": {
    "caseStudyCategory": {
      "type": "enumeration",
      "enum": ["strategy", "performance"]
    },
    "isBigCard": {
      "type": "boolean",
      "default": true
    },
    "carouselOptions": {
      "type": "component",
      "repeatable": false,
      "component": "slider.carousel-options"
    },
    "sectionPadding": {
      "type": "component",
      "repeatable": false,
      "component": "config.section-padding"
    },
    "title": {
      "type": "text"
    },
    "subTitle": {
      "type": "string"
    },
    "caseStudyList": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case-study-list.case-study-list"
    }
  }
}
